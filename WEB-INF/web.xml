<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <description>The standard web descriptor for the html adaptor</description>
    <listener>
        <listener-class>ru.comita.ccee.front2.listeners.HttpSessionTimeoutListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>Servlet</servlet-name>
        <servlet-class>ru.comita.ors.server.Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Servlet</servlet-name>
        <url-pattern>/main</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>30</session-timeout>
    </session-config>
</web-app>
